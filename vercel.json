{
    "functions": {
      "dist/server/pages/api/og-image.js": {
        "maxDuration": 10,
        "memory": 1024
      },
      "dist/server/pages/api/sentiment.js": {
        "maxDuration": 30,
        "memory": 1024
      },
      "dist/server/pages/api/comments.js": {
        "maxDuration": 30,
        "memory": 1024
      },
      "dist/server/pages/api/save-result.js": {
        "maxDuration": 10,
        "memory": 512
      }
    },
    "headers": [
      {
        "source": "/api/og-image",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400, s-maxage=86400, stale-while-revalidate=604800"
          },
          {
            "key": "Content-Type",
            "value": "image/svg+xml"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, OPTIONS"
          }
        ]
      },
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type, Authorization"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/og-image",
        "destination": "/api/og-image"
      }
    ]
  }
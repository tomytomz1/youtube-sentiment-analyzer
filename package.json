{
  "name": "youtube-sentiment-analyzer",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.astro --fix",
    "typecheck": "astro check && tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro,js,jsx,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:reddit-sentiment": "vitest run src/tests/reddit-sentiment.test.ts",
    "validate": "npm run typecheck && npm run lint && npm run test:coverage",
    "pre-commit": "npm run format && npm run validate",
    "openapi:validate": "swagger-codegen validate -i openapi.yaml",
    "openapi:docs": "swagger-codegen generate -i openapi.yaml -l html2 -o docs/api"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.3.0",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/tailwind": "^5.1.3",
    "@astrojs/vercel": "^8.0.4",
    "@types/react-dom": "^19.1.6",
    "@upstash/redis": "^1.35.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.7.2",
    "astro": "^5.11.0",
    "dotenv": "^17.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4"
  }
}
